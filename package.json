{
  "name": "my-qwik-empty-starter",
  "description": "Blank project with routing included",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "type": "module",
  "scripts": {
    "postinstall": "if [ -z \"$SKIP_POSTINSTALL\" ] || [ \"$SKIP_POSTINSTALL\" = \"0\" ]; then npx playwright install; else echo 'Skipping playwright installation.'; fi",
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/express/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "lint": "biome lint . --write ",
    "lint.check": "biome lint .",
    "fmt": "biome format . --write",
    "fmt.check": "biome format .",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node server/entry.express",
    "start": "vite --open --mode ssr",
    "test": "vitest",
    "test.unit.ui": "vitest --ui",
    "qwik": "qwik",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@builder.io/qwik": "^1.16.0",
    "@builder.io/qwik-city": "^1.16.0",
    "@qwik-ui/headless": "^0.7.0",
    "@qwik-ui/styled": "^0.3.4",
    "@qwik-ui/utils": "^0.3.2",
    "@qwikest/icons": "^0.0.13",
    "@tanstack/virtual-core": "^3.13.6",
    "@types/compression": "^1.7.5",
    "@types/eslint": "9.6.1",
    "@types/express": "^5.0.3",
    "@types/marked": "^6.0.0",
    "@types/mustache": "^4.2.5",
    "@types/node": "24.3.0",
    "@typescript-eslint/eslint-plugin": "8.41.0",
    "@typescript-eslint/parser": "8.41.0",
    "@vitest/ui": "^3.0.7",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "compression": "^1.8.0",
    "dotenv": "^17.2.1",
    "husky": "^9.1.7",
    "nx": "^21.4.1",
    "postcss": "^8.5.3",
    "qwik-ui": "^0.3.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.9.2",
    "undici": "^7.3.0",
    "vite": "7.1.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.7"
  },
  "dependencies": {
    "@duckdb/node-api": "1.2.2-alpha.19",
    "@huggingface/hub": "~2.5.0",
    "@huggingface/inference": "~4.7.1",
    "@huggingface/tasks": "^0.19.5",
    "@huggingface/transformers": "^3.5.1",
    "@lancedb/lancedb": "^0.18.2",
    "apache-arrow": "^21.0.0",
    "consola": "^3.4.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "express": "5.1.0",
    "highlight.js": "^11.11.1",
    "marked": "^16.0.0",
    "marked-highlight": "^2.2.2",
    "marked-katex-extension": "^5.1.4",
    "mustache": "^4.2.0",
    "node-cache": "^5.1.2",
    "playwright": "^1.51.1",
    "playwright-core": "^1.51.1",
    "sbd": "^1.0.19",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7",
    "yaml": "^2.7.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "nx",
      "onnxruntime-node",
      "protobufjs",
      "sharp",
      "sqlite3"
    ]
  }
}
